# Copy this to .env.local and fill in your actual values

# Email Service (Resend - recommended)
RESEND_API_KEY=your_resend_api_key_here
ADMIN_EMAIL=your_admin_email@gmail.com

# Alternative: SendGrid
# SENDGRID_API_KEY=your_sendgrid_api_key_here

# Airtable (for lead management)
AIRTABLE_BASE_ID=your_airtable_base_id
AIRTABLE_API_KEY=your_airtable_api_key

# Alternative: Google Sheets
# GOOGLE_SHEETS_PRIVATE_KEY=your_google_sheets_key
# GOOGLE_SHEETS_CLIENT_EMAIL=your_service_account_email
# GOOGLE_SPREADSHEET_ID=your_spreadsheet_id

# Analytics (optional)
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
FACEBOOK_PIXEL_ID=your_facebook_pixel_id

# NextAuth Configuration
NEXTAUTH_URL=https://delphincleanair.org
NEXTAUTH_SECRET=your_nextauth_secret_here_generate_a_random_string

# Google OAuth (for admin authentication)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
